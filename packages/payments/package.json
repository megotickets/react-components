{
  "name": "@megotickets/payments",
  "private": false,
  "version": "1.0.2",
  "description": "Payment processing components for MegoTickets React components library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.tsx --format cjs,esm --dts --external react --inject-style --tsconfig tsconfig.json",
    "dev": "tsup src/index.tsx --format cjs,esm --dts --external react --inject-style --watch --tsconfig tsconfig.json",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@megotickets/core": "^1.0.2",
    "@react-google-maps/api": "^2.20.6",
    "@stripe/stripe-js": "^5.8.0",
    "axios": "^1.7.7",
    "styled-components": "^6.1.16"
  },
  "keywords": ["react", "components", "payments", "stripe", "crypto", "megotickets"],
  "author": "MegoTickets",
  "license": "MIT",
  "packageManager": "pnpm@10.4.1",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/megotickets/react-components.git",
    "directory": "packages/payments"
  },
  "homepage": "https://github.com/megotickets/react-components#readme",
  "bugs": {
    "url": "https://github.com/megotickets/react-components/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.0.0"
  },
  "sideEffects": ["**/*.css"]
}
